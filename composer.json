{
    "name": "keerill/laravel-http-client-logger",
    "description": "Logger Request and responses HTTP client for laravel",
    "keywords": [
        "popkovkirill",
        "laravel",
        "laravel-http-client-logger",
        "log",
        "logging",
        "psr3"
    ],
    "homepage": "https://github.com/keerill/laravel-http-client-logger",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill Popkov",
            "email": "i@popkovkirill.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^7.0|^8.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0"
    },
    "autoload": {
        "psr-4": {
            "Keerill\\HttpLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keerill\\HttpLogger\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-http-client-logger --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
